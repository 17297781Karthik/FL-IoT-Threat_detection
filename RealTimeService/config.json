{
  "paths": {
    "pcap_directory": "../samplePackets/pcaps",
    "model_path": "SavedGlobalModel/final_model.pth",
    
    "results_directory": "detection_results",
    "log_file": "iot_threat_detection.log"
  },
  "model": {
    "num_features": 17,
    "num_classes": 10,
    "feature_names": [
      "HH_jit_L1_variance",
      "HH_jit_L0.01_variance", 
      "HH_jit_L3_variance",
      "HH_jit_L0.1_variance",
      "HH_jit_L0.1_mean",
      "H_L0.01_variance",
      "HH_jit_L5_variance",
      "MI_dir_L0.01_variance",
      "MI_dir_L0.1_mean",
      "MI_dir_L0.01_weight",
      "HH_L0.01_std",
      "MI_dir_L0.1_weight",
      "H_L0.01_mean",
      "H_L0.01_weight",
      "HH_jit_L5_mean",
      "MI_dir_L1_weight",
      "H_L0.1_weight"
    ]
  },
  "attack_types": {
    "0": "Benign",
    "1": "Gafgyt Combo",
    "2": "Gafgyt Junk",
    "3": "Gafgyt TCP",
    "5": "Mirai ACK",
    "6": "Mirai Scan",
    "7": "Mirai SYN",
    "8": "Mirai UDP",
    "9": "Mirai UDPPlain"
  },
  "processing": {
    "lambda_values": [0.01, 0.1, 1, 3, 5],
    "batch_size": 32,
    "confidence_threshold": 0.5,
    "high_confidence_threshold": 0.8
  },
  "feature_extraction": {
    "flow_timeout": 600,
    "min_packets_per_flow": 2,
    "max_flows_per_file": 10000
  },
  "output": {
    "save_features": true,
    "save_predictions": true,
    "generate_reports": true,
    "output_format": "json"
  },
  "logging": {
    "level": "INFO",
    "console_output": true,
    "file_output": true,
    "max_log_size": "10MB",
    "backup_count": 5
  }
}
